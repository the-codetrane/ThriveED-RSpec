<!DOCTYPE html>
<html>
  <head>
    <title>ThriveEdRspec</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% if flash.present? %>
    <% flash.each do |type, msg| %>
      <p id="<%= type %>"><%= msg %></p>
    <% end %>
  <% end  %>
  <%= yield %>
  </body>
</html>
